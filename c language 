include <stdio.h>

int main() 
{
    int i,j;
    for(i=1;i<=3;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("* ");
        }
        printf("\n");
    }
    for(i=3;i>=0;i--)
    {
        for(j=0;j<=i;j++)
        {
            printf("* ");
        }
        printf("\n");
    }
    
    
    return 0;
}
#include <stdio.h>

int main()
{
    int i,j,n=1;
    for(i=0;i<4;i++)
    {
       for(j=0;j<4;j++) 
       {
           printf("%d ",n);
           n++;
       }
       printf("\n");
    }
    return 0;
}